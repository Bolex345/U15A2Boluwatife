<script>
    // This function handles going back in browser history
    function goBack() {
        if (document.referrer !== '') {
            history.back();
        } else {
            window.location.href = "index.html"; // Default to homepage if there's no history
        }
    }
</script>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
